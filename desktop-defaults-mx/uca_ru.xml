<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
   <icon></icon>
   <name>Выполнить команду ...</name>
   <unique-id>1391443541044100-3</unique-id>
   <command>xfce4-appfinder --collapsed</command>
   <description>Запуск программы</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon>utilities-terminal</icon>
   <name>Открыть терминал здесь</name>
   <unique-id>1388590880939619-1</unique-id>
   <command>exo-open --working-directory %f --launch TerminalEmulator</command>
   <description>Запуск эмулятора терминала здесь</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Открыть root терминал здесь</name>
   <unique-id>1391443434179984-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu thunar %f</command>
   <description>Открытие папки с правами администратора</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Передать права Root</name>
   <unique-id>1388696949105908-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R root:root %F&quot;</command>
   <description>Изменение административных разрешений</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Передать права пользователю</name>
   <unique-id>1388702559256309-5</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R $USER:$USER %F&quot;</command>
   <description>Удаление ограничений использования</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Редактировать от Root</name>
   <unique-id>1389150009142611-4</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu featherpad %f</command>
   <description>Внесение изменений в текстовые файлы</description>
   <patterns>*</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Найти файлы здесь</name>
   <unique-id>1391015829778456-1</unique-id>
   <command>catfish --path=%f</command>
   <description>Поиск в текущей папке</description>
   <patterns>*</patterns>
   <directories/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Создать символьную ссылку</name>
   <unique-id>1394124450909421-1</unique-id>
   <command>ln -s %f &quot;%n (symlink)&quot;</command>
   <description>Ссылка между двумя файлами или директориями</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon>folder-publicshare</icon>
   <name>Сделать папку общей в вашей сети</name>
   <unique-id>1399025775400525-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu system-config-samba %d</command>
   <description>Настройка совместного использования файлов</description>
   <patterns>*</patterns>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Посчитать md5sum</name>
   <unique-id>1426501025005582-4</unique-id>
   <command>yad --info --title=&quot;Check md5sum for %n&quot; --text=&quot;$(md5sum %n)&quot; | yad --width=350 --height=250 --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Calculating md5sum for %n&quot;</command>
   <description>Показ контрольной суммы ISO</description>
   <patterns>*.iso;*.ISO</patterns>
   <other-files/>
</action>
<action>
   <icon></icon>
   <name>Проверка целостности данных</name>
   <unique-id>1426501047462724-5</unique-id>
   <command>yad --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum -c %f)&quot; | yad --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Checking md5sum for %n&quot;</command>
   <description>Сравнение файлов ISO и md5sum</description>
   <patterns>*.md5;*.md5sum</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
	<icon></icon>
	<name>Создать звук при входе</name>
	<unique-id>1447629663342880-3</unique-id>
	<command>echo %f &gt;~/.config/startupsound.conf</command>
	<description>Назначение выбранного звука для входа</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
<action>
	<icon></icon>
	<name>Создать звук при выходе</name>
	<unique-id>1447629663342880-4</unique-id>
	<command>echo %f &gt;~/.config/logoutsound.conf</command>
	<description>Назначение выбранного звука для выхода</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
</actions>
