<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
   <icon></icon>
   <name>Esegui comando ...</name>
   <unique-id>1391443541044100-3</unique-id>
   <command>xfce4-appfinder --collapsed</command>
   <description>Lancia un programma</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon>utilities-terminal</icon>
   <name>Apri il terminale qui</name>
   <unique-id>1388590880939619-1</unique-id>
   <command>exo-open --working-directory %f --launch TerminalEmulator</command>
   <description>Lancia l'emulatore di terminale qui</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Apri root Thunar qui</name>
   <unique-id>1391443434179984-1</unique-id>
   <command>gksu thunar %f</command>
   <description>Apri questa cartella con privilegi amministrativi</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Proprietà a Root</name>
   <unique-id>1388696949105908-1</unique-id>
   <command>gksu &quot;chown -R root:root %F&quot;</command>
   <description>Modifica i permessi a favore dell'amministratore</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Proprietà all'utente</name>
   <unique-id>1388702559256309-5</unique-id>
   <command>gksu &quot;chown -R $USER:$USER %F&quot;</command>
   <description>Rimuove le restrizioni verso l'utente</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Edita come Root</name>
   <unique-id>1389150009142611-4</unique-id>
   <command>gksu featherpad %f</command>
   <description>Fare modifiche amministrative ai file di testo</description>
   <patterns>*</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Trova file qui</name>
   <unique-id>1391015829778456-1</unique-id>
   <command>catfish --path=%f</command>
   <description>Fare una ricerca sulla cartella corrente</description>
   <patterns>*</patterns>
   <directories/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Crea symlink</name>
   <unique-id>1394124450909421-1</unique-id>
   <command>ln -s %f &quot;%n (symlink)&quot;</command>
   <description>Creare un link simbolico tra due file o directory</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon>folder-publicshare</icon>
   <name>Condividi una cartella sulla tua rete</name>
   <unique-id>1399025775400525-1</unique-id>
   <command>gksu system-config-samba %d</command>
   <description>Configurare la condivisione dei file</description>
   <patterns>*</patterns>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Computa l'md5sum</name>
   <unique-id>1426501025005582-4</unique-id>
   <command>yad --info --title=&quot;Check md5sum for %n&quot; --text=&quot;$(md5sum %n)&quot; | yad --width=350 --height=250 --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Calculating md5sum for %n&quot;</command>
   <description>Visualizzare la somma md5 dell'ISO</description>
   <patterns>*.iso;*.ISO</patterns>
   <other-files/>
</action>
<action>
   <icon></icon>
   <name>Controlla l'integrità dei dati</name>
   <unique-id>1426501047462724-5</unique-id>
   <command>yad --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum -c %f)&quot; | yad --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Checking md5sum for %n&quot;</command>
   <description>Comparare tra loro i file ISO e md5sum</description>
   <patterns>*.md5;*.md5sum</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
	<icon></icon>
	<name>Imposta il suono di avvio</name>
	<unique-id>1447629663342880-3</unique-id>
	<command>echo %f &gt;~/.config/startupsound.conf</command>
	<description>Impostare il suono di avvio selezionato</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
<action>
	<icon></icon>
	<name>Imposta il suono di uscita</name>
	<unique-id>1447629663342880-4</unique-id>
	<command>echo %f &gt;~/.config/logoutsound.conf</command>
	<description>Impostare il suono di uscita selezionato</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
</actions>
