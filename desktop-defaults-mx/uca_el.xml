<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
   <icon></icon>
   <name>Εκτέλεση εντολής ...</name>
   <unique-id>1391443541044100-3</unique-id>
   <command>xfce4-appfinder --collapsed</command>
   <description>Ξεκινήστε ένα πρόγραμμα</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon>utilities-terminal</icon>
   <name>Ανοίξτε το τερματικό εδώ</name>
   <unique-id>1388590880939619-1</unique-id>
   <command>exo-open --working-directory %f --launch TerminalEmulator</command>
   <description>Εκκινήστε τον εξομοιωτή τερματικού εδώ</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Ανοίξτε το Thunar ως root εδώ</name>
   <unique-id>1391443434179984-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu thunar %f</command>
   <description>Ανοίξτε αυτό το φάκελο με δικαιώματα διαχειριστή</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Ιδιοκτησία στο root</name>
   <unique-id>1388696949105908-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R root:root %F&quot;</command>
   <description>Αλλαγή στα δικαιώματα διαχειριστή</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Ιδιοκτησία προς τον χρήστη</name>
   <unique-id>1388702559256309-5</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R $USER:$USER %F&quot;</command>
   <description>Καταργεί τους περιορισμούς χρήσης</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Επεξεργασία ως Root</name>
   <unique-id>1389150009142611-4</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu featherpad %f</command>
   <description>Κάντε αλλαγές διαχείρισης σε αρχεία κειμένου</description>
   <patterns>*</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Βρείτε αρχεία εδώ</name>
   <unique-id>1391015829778456-1</unique-id>
   <command>catfish --path=%f</command>
   <description>Αναζήτηση τρέχοντος φακέλου</description>
   <patterns>*</patterns>
   <directories/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Δημιουργία σύζευξης</name>
   <unique-id>1394124450909421-1</unique-id>
   <command>ln -s %f &quot;%n (symlink)&quot;</command>
   <description>Softlink μεταξύ δύο αρχείων ή καταλόγων</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon>folder-publicshare</icon>
   <name>Μοιραστείτε ένα φάκελο στο δίκτυό σας</name>
   <unique-id>1399025775400525-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu system-config-samba %d</command>
   <description>Διαμορφώστε την κοινή χρήση αρχείων</description>
   <patterns>*</patterns>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Υπολογίστε το md5sum</name>
   <unique-id>1426501025005582-4</unique-id>
   <command>yad --info --title=&quot;Check md5sum for %n&quot; --text=&quot;$(md5sum %n)&quot; | yad --width=350 --height=250 --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Calculating md5sum for %n&quot;</command>
   <description>Εμφάνιση το άθροισμα ελέγχου του ISO</description>
   <patterns>*.iso;*.ISO</patterns>
   <other-files/>
</action>
<action>
   <icon></icon>
   <name>Ελέγξτε την ακεραιότητα των δεδομένων</name>
   <unique-id>1426501047462724-5</unique-id>
   <command>yad --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum -c %f)&quot; | yad --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Checking md5sum for %n&quot;</command>
   <description>Συγκρίνετε τα αρχεία ISO και md5sum</description>
   <patterns>*.md5;*.md5sum</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
	<icon></icon>
	<name>ήχο εκκίνησης</name>
	<unique-id>1447629663342880-3</unique-id>
	<command>echo %f &gt;~/.config/startupsound.conf</command>
	<description>επιλεγμένη εκκίνηση ήχου</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
<action>
	<icon></icon>
	<name>ήχο αποσύνδεσης</name>
	<unique-id>1447629663342880-4</unique-id>
	<command>echo %f &gt;~/.config/logoutsound.conf</command>
	<description>επιλεγμένο ήχο αποσύνδεσης</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
</actions>
