<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
   <icon></icon>
   <name>Commando uitvoeren ...</name>
   <unique-id>1391443541044100-3</unique-id>
   <command>xfce4-appfinder --collapsed</command>
   <description>Programma starten</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon>utilities-terminal</icon>
   <name>Open Terminal Hier</name>
   <unique-id>1388590880939619-1</unique-id>
   <command>exo-open --working-directory %f --launch TerminalEmulator</command>
   <description>Start terminal emulator hier</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Open root Thunar hier</name>
   <unique-id>1391443434179984-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu thunar %f</command>
   <description>Open deze map met administratieve rechten</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Eigenaarschap naar Root</name>
   <unique-id>1388696949105908-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R root:root %F&quot;</command>
   <description>Verander naar administratieve permissies</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Eigenaarschap naar gebruiker</name>
   <unique-id>1388702559256309-5</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R $USER:$USER %F&quot;</command>
   <description>Verwijdert gebruikersrestricties</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Aanpassen als Root</name>
   <unique-id>1389150009142611-4</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu featherpad %f</command>
   <description>Maak administratieve veranderingen naar tekstbestanden</description>
   <patterns>*</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Vind bestanden hier</name>
   <unique-id>1391015829778456-1</unique-id>
   <command>catfish --path=%f</command>
   <description>Zoeken in huidige map</description>
   <patterns>*</patterns>
   <directories/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>CreÃ«er symlink</name>
   <unique-id>1394124450909421-1</unique-id>
   <command>ln -s %f &quot;%n (symlink)&quot;</command>
   <description>Softlink tussen twee bestanden of directories</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon>folder-publicshare</icon>
   <name>Deel een folder op je netwerk</name>
   <unique-id>1399025775400525-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu system-config-samba %d</command>
   <description>Configureer bestanddeling</description>
   <patterns>*</patterns>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Bereken md5sum</name>
   <unique-id>1426501025005582-4</unique-id>
   <command>yad --info --title=&quot;Check md5sum for %n&quot; --text=&quot;$(md5sum %n)&quot; | yad --width=350 --height=250 --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Calculating md5sum for %n&quot;</command>
   <description>Laat checksum van ISO zien</description>
   <patterns>*.iso;*.ISO</patterns>
   <other-files/>
</action>
<action>
   <icon></icon>
   <name>Controleer data integriteit</name>
   <unique-id>1426501047462724-5</unique-id>
   <command>yad --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum -c %f)&quot; | yad --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Checking md5sum for %n&quot;</command>
   <description>Vergelijk ISO en md5sum bestanden</description>
   <patterns>*.md5;*.md5sum</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
	<icon></icon>
