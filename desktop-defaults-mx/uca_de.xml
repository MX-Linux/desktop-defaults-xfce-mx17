<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
   <icon></icon>
   <name>Befehl ausführen ...</name>
   <unique-id>1391443541044100-3</unique-id>
   <command>xfce4-appfinder --collapsed</command>
   <description>Ein Programm starten</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon>utilities-terminal</icon>
   <name>Terminal hier öffnen</name>
   <unique-id>1388590880939619-1</unique-id>
   <command>exo-open --working-directory %f --launch TerminalEmulator</command>
   <description>Einen Terminal-Emulator hier starten</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Thunar als Root hier öffnen</name>
   <unique-id>1391443434179984-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu thunar %f</command>
   <description>Dieses Verzeichnis mit Administratorenrechten öffnen</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Eigentümerschaft zu Root</name>
   <unique-id>1388696949105908-1</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R root:root %F&quot;</command>
   <description>Auf administrative Berechtigungen ändern</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Eigentümerschaft zum Benutzer</name>
   <unique-id>1388702559256309-5</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu &quot;chown -R $USER:$USER %F&quot;</command>
   <description>Entfernt Beschränkungen der Nutzung</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Als Root bearbeiten</name>
   <unique-id>1389150009142611-4</unique-id>
   <command>gnome-keyring-daemon -r -d;gksu featherpad %f</command>
   <description>Administrative Änderungen an Textdateien vornehmen</description>
   <patterns>*</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Dateien hier finden</name>
   <unique-id>1391015829778456-1</unique-id>
   <command>catfish --path=%f</command>
   <description>Aktuelles Verzeichnis durchsuchen</description>
   <patterns>*</patterns>
   <directories/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Symbolische Verknüpfung erstellen</name>
   <unique-id>1394124450909421-1</unique-id>
   <command>ln -s %f &quot;%n (symlink)&quot;</command>
   <description>Softlink zwischen zwei Dateien oder Ordnern erstellen</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon>folder-publicshare</icon>
   <name>Ein Verzeichnis in deinem Netzwerk teilen</name>
   <unique-id>1399025775400525-1</unique-id>
   <command>gksu system-config-samba %d</command>
   <description>Dateifreigaben konfigurieren</description>
   <patterns>*</patterns>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Berechne md5sum</name>
   <unique-id>1426501025005582-4</unique-id>
   <command>yad --info --title=&quot;Check md5sum for %n&quot; --text=&quot;$(md5sum %n)&quot; | yad --width=350 --height=250 --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Calculating md5sum for %n&quot;</command>
   <description>Prüfsumme einer ISO anzeigen</description>
   <patterns>*.iso;*.ISO</patterns>
   <other-files/>
</action>
<action>
   <icon></icon>
   <name>Datenintegrität prüfen</name>
   <unique-id>1426501047462724-5</unique-id>
   <command>yad --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum -c %f)&quot; | yad --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Checking md5sum for %n&quot;</command>
   <description>Vergleiche ISO- und md5sum-Dateien</description>
   <patterns>*.md5;*.md5sum</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
	<icon></icon>
	<name>Startsound festlegen</name>
	<unique-id>1447629663342880-3</unique-id>
	<command>echo %f &gt;~/.config/startupsound.conf</command>
	<description>Gewählten Sound für den Start verwenden</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
<action>
	<icon></icon>
	<name>Abmeldesound festlegen</name>
	<unique-id>1447629663342880-4</unique-id>
	<command>echo %f &gt;~/.config/logoutsound.conf</command>
	<description>Gewählten Sound beim Abmelden verwenden</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
</actions>
