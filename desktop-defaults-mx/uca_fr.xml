<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
   <icon></icon>
   <name>Exécuter une commande ...</name>
   <unique-id>1391443541044100-3</unique-id>
   <command>xfce4-appfinder --collapsed</command>
   <description>Démarrer un programme</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon>utilities-terminal</icon>
   <name>Ouvrir un terminal ici</name>
   <unique-id>1388590880939619-1</unique-id>
   <command>exo-open --working-directory %f --launch TerminalEmulator</command>
   <description>Lancer un terminal ici</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Ouvrir ici Thunar en compte root</name>
   <unique-id>1391443434179984-1</unique-id>
   <command>gksu thunar %f</command>
   <description>Ouvrir ce répertoire avec les privilèges administrateurs</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Propriété passée à root</name>
   <unique-id>1388696949105908-1</unique-id>
   <command>gksu &quot;chown -R root:root %F&quot;</command>
   <description>Passer en privilèges administrateurs</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Propriété passée à l'utilisateur</name>
   <unique-id>1388702559256309-5</unique-id>
   <command>gksu &quot;chown -R $USER:$USER %F&quot;</command>
   <description>Lever les restrictions d'utilisation</description>
   <patterns>*</patterns>
   <directories/>
   <audio-files/>
   <image-files/>
   <other-files/>
   <text-files/>
   <video-files/>
</action>
<action>
   <icon></icon>
   <name>Éditer en tant que root</name>
   <unique-id>1389150009142611-4</unique-id>
   <command>gksu featherpad %f</command>
   <description>Opérer des changements administrateur sur des fichiers textes</description>
   <patterns>*</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Trouver des fichiers ici</name>
   <unique-id>1391015829778456-1</unique-id>
   <command>catfish --path=%f</command>
   <description>Faire une recherche dans le répertoire actuel</description>
   <patterns>*</patterns>
   <directories/>
   <text-files/>
</action>
<action>
   <icon></icon>
   <name>Créer un lien symbolique</name>
   <unique-id>1394124450909421-1</unique-id>
   <command>ln -s %f &quot;%n (symlink)&quot;</command>
   <description>Lien symbolique entre deux fichiers ou répertoires</description>
   <patterns>*</patterns>
   <startup-notify/>
   <directories/>
   <other-files/>
   <text-files/>
</action>
<action>
   <icon>folder-publicshare</icon>
   <name>Partagez un dossier sur votre réseau</name>
   <unique-id>1399025775400525-1</unique-id>
   <command>gksu system-config-samba %d</command>
   <description>Configurer le partage de fichiers</description>
   <patterns>*</patterns>
   <directories/>
</action>
<action>
   <icon></icon>
   <name>Calculer la somme de contrôle md5</name>
   <unique-id>1426501025005582-4</unique-id>
   <command>yad --info --title=&quot;Check md5sum for %n&quot; --text=&quot;$(md5sum %n)&quot; | yad --width=350 --height=250 --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Calculating md5sum for %n&quot;</command>
   <description>Afficher la somme de contrôle de l'ISO</description>
   <patterns>*.iso;*.ISO</patterns>
   <other-files/>
</action>
<action>
   <icon></icon>
   <name>Contrôler l'intégrité des données</name>
   <unique-id>1426501047462724-5</unique-id>
   <command>yad --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum -c %f)&quot; | yad --progress --percentage=&quot;50&quot; --auto-kill --auto-close --title=&quot;Checksum utility&quot; --text=&quot;Checking md5sum for %n&quot;</command>
   <description>Comparer les fichiers ISO et somme de contrôle md5</description>
   <patterns>*.md5;*.md5sum</patterns>
   <other-files/>
   <text-files/>
</action>
<action>
	<icon></icon>
	<name>Choisir comme son de connexion</name>
	<unique-id>1447629663342880-3</unique-id>
	<command>echo %f &gt;~/.config/startupsound.conf</command>
	<description>Le fichier sélectionné devient le son de connexion</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
<action>
	<icon></icon>
	<name>Choisir comme son de déconnexion</name>
	<unique-id>1447629663342880-4</unique-id>
	<command>echo %f &gt;~/.config/logoutsound.conf</command>
	<description>Le fichier sélectionné devient le son de déconnexion</description>
	<patterns>*</patterns>
	<audio-files/>
</action>
</actions>
